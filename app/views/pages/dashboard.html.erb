 <%= render 'layouts/header' %>
                
            <div class="row">
                <div class="col-lg-12">
                    <h5 class="center">Dashboard <%= link_to "Create New Cinema", new_listing_path, class: "btn btn-danger" %></h5>
                  
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
         
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h5 class="center">My Current Movie Listings</h5>
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>Movie</th>
                                            <th>Date</th>
                                            <th>Time</th>
                                            <th>Seats</th>
                                            <th>Venue Name</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                      <% @listings.each do |listing| %>
        <tr>
          <td><%= link_to listing.title, listing_path(listing) %></td>
          <td><%= listing.date %></td>
          <td><%= listing.time %></td>
          <td><%= listing.seats %></td>
          <td><%= listing.venue_name %></td>
          
          <td><%= link_to "Edit Movie", edit_listing_path(listing) %></td>
          
        </tr>
        <% end %>
      </tbody>
</table>  
                                    
